<div class="breadcrumb">
<div id="form-heading">
  <h3>Listing feedbacks</h3><%= image_tag 'divider.png' %>
</div>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Feedback</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.name %></td>
        <td><%= feedback.feedback %></td>
        <td><%= link_to 'Show', feedback, :class => 'btn btn-mini btn-info' %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback), :class => 'btn btn-mini' %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-mini btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<center><%= link_to 'New Feedback', new_feedback_path, :class => 'btn btn-primary' %></center>
</div>