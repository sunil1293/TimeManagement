
  <thead>
    <tr>
      <th><h3>TimeOff Details :</h3><th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td>Leave Type :</td>
        <td><%= @time_off.leave_type %></td>
      </tr>
      <tr>
        <td>Type :</td>
        <td><%= @time_off.type %></td>
      </tr>
      </tr>
        <td>Date :</td>
        <td><%= @time_off.date %></td>
      </tr>
      <tr>
        <td>Number Of Hours:</td>
        <td><%= @time_off.number_of_hours %></td>
      </tr>
        <td>From :</td>
        <td><%= @time_off.from %></td>
      </tr>
      </tr>
        <td>To :</td>
        <td><%= @time_off.to %></td>
      </tr>
      <tr>
        <td>Reason :</td>
        <td><%= @time_off.reason %></td>
      </tr>
      <tr>
      <td>Status :</td>
        <% if @time_off.status == 'New' %>
          <td><span class="label label-info"><%= @time_off.status %></span></td>
        <% elsif @time_off.status == 'Approved' %>
        <td><span class="label label-success"><%= @time_off.status %></span></td>
        <% else %>
        <td><span class="label label-important"><%= @time_off.status %></span></td>
        <% end %>
      </tr>
  </tbody>

